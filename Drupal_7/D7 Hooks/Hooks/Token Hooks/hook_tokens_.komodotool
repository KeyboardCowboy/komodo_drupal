  "name": "hook_tokens()", 
  "value": [
    "/**", 
    " * Implements hook_tokens().", 
    " */", 
    "function [[%b]]_tokens($type, $tokens, array $data = array(), array $options = array()) {", 
    "  $replacements = array();", 
    "  ", 
    "  switch ($type) {", 
    "    case '!@#_currentPos!@#_anchor':", 
    "      foreach ($tokens as $name => $original) {", 
    "        switch ($name) {", 
    "          case '':", 
    "            $replacements[$original] = '';", 
    "            break;", 
    "        }", 
    "      }", 
    "      break;", 
    "  }", 
    "  ", 
    "  return $replacements;", 
    "}"
  ], 
